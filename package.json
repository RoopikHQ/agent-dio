{
	"name": "roodio",
	"displayName": "Dio",
	"description": "AI Coding Agent with Roopik IDE Integration",
	"publisher": "roopik",
	"version": "3.45.1",
	"icon": "dist/assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.19.2"
	},
	"author": {
		"name": "Roopik"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RoopikHQ/roopik"
	},
	"homepage": "https://roopik.com/",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"dio code",
		"dio-code",
		"roopik-labs",
		"dio-ide",
		"dio-dio",
		"dio"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "roodio-ActivityBar",
					"title": "%views.activitybar.title%",
					"icon": "dist/assets/icons/icon.svg"
				}
			],
			"secondarySidebar": [
				{
					"id": "roodio-AuxiliaryBar",
					"title": "Dio",
					"icon": "dist/assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"roodio-ActivityBar": [
				{
					"type": "webview",
					"id": "roodio.SidebarProvider",
					"name": "%views.sidebar.name%"
				}
			],
			"roodio-AuxiliaryBar": [
				{
					"type": "webview",
					"id": "roodio.ChatPanel",
					"name": "Chat"
				}
			]
		},
		"commands": [
			{
				"command": "roodio.openChatPanel",
				"title": "Open Roopik Dio Chat",
				"category": "Roopik Dio",
				"icon": {
					"light": "dist/assets/icons/dio-icon-light.svg",
					"dark": "dist/assets/icons/dio-icon-dark.svg"
				}
			},
			{
				"command": "roodio.plusButtonClicked",
				"title": "%command.newTask.title%",
				"icon": "$(edit)"
			},
			{
				"command": "roodio.historyButtonClicked",
				"title": "%command.history.title%",
				"icon": "$(history)"
			},
			{
				"command": "roodio.marketplaceButtonClicked",
				"title": "%command.marketplace.title%",
				"icon": "$(extensions)"
			},
			{
				"command": "roodio.popoutButtonClicked",
				"title": "%command.openInEditor.title%",
				"icon": "$(link-external)"
			},
			{
				"command": "roodio.cloudButtonClicked",
				"title": "%command.cloud.title%",
				"icon": "$(cloud)"
			},
			{
				"command": "roodio.settingsButtonClicked",
				"title": "%command.settings.title%",
				"icon": "$(settings-gear)"
			},
			{
				"command": "roodio.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.externalContext",
				"title": "Add External Context to Dio",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "Terminal"
			},
			{
				"command": "roodio.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roodio.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roodio.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.importSettings",
				"title": "%command.importSettings.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.focusInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roodio.toggleAutoApprove",
				"title": "%command.toggleAutoApprove.title%",
				"category": "%configuration.title%"
			}
		],
		"menus": {
			"layoutControl": [
				{
					"command": "roodio.openChatPanel",
					"group": "navigation",
					"order": 0
				}
			],
			"editor/context": [
				{
					"submenu": "roodio.contextMenu",
					"group": "1"
				}
			],
			"roodio.contextMenu": [
				{
					"command": "roodio.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roodio.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "roodio.improveCode",
					"group": "1_actions@3"
				},
				{
					"command": "roodio.fixCode",
					"group": "1_actions@4"
				}
			],
			"terminal/context": [
				{
					"submenu": "roodio.terminalMenu",
					"group": "2"
				}
			],
			"roodio.terminalMenu": [
				{
					"command": "roodio.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roodio.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "roodio.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "roodio.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == roodio.SidebarProvider"
				},
				{
					"command": "roodio.settingsButtonClicked",
					"group": "navigation@2",
					"when": "view == roodio.SidebarProvider"
				},
				{
					"command": "roodio.marketplaceButtonClicked",
					"group": "navigation@3",
					"when": "view == roodio.SidebarProvider"
				},
				{
					"command": "roodio.historyButtonClicked",
					"group": "overflow@1",
					"when": "view == roodio.SidebarProvider"
				},
				{
					"command": "roodio.popoutButtonClicked",
					"group": "overflow@2",
					"when": "view == roodio.SidebarProvider"
				},
				{
					"command": "roodio.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == roodio.ChatPanel"
				},
				{
					"command": "roodio.settingsButtonClicked",
					"group": "navigation@2",
					"when": "view == roodio.ChatPanel"
				},
				{
					"command": "roodio.marketplaceButtonClicked",
					"group": "navigation@3",
					"when": "view == roodio.ChatPanel"
				},
				{
					"command": "roodio.historyButtonClicked",
					"group": "overflow@1",
					"when": "view == roodio.ChatPanel"
				},
				{
					"command": "roodio.popoutButtonClicked",
					"group": "overflow@2",
					"when": "view == roodio.ChatPanel"
				}
			],
			"editor/title": [
				{
					"command": "roodio.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == roodio.TabPanelProvider"
				},
				{
					"command": "roodio.settingsButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == roodio.TabPanelProvider"
				},
				{
					"command": "roodio.marketplaceButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == roodio.TabPanelProvider"
				},
				{
					"command": "roodio.historyButtonClicked",
					"group": "overflow@1",
					"when": "activeWebviewPanelId == roodio.TabPanelProvider"
				},
				{
					"command": "roodio.popoutButtonClicked",
					"group": "overflow@2",
					"when": "activeWebviewPanelId == roodio.TabPanelProvider"
				}
			]
		},
		"keybindings": [
			{
				"command": "roodio.openChatPanel",
				"key": "ctrl+alt+b",
				"mac": "cmd+alt+b",
				"win": "ctrl+alt+b",
				"linux": "ctrl+alt+b"
			},
			{
				"command": "roodio.addToContext",
				"key": "cmd+k cmd+a",
				"mac": "cmd+k cmd+a",
				"win": "ctrl+k ctrl+a",
				"linux": "ctrl+k ctrl+a",
				"when": "editorTextFocus && editorHasSelection"
			},
			{
				"command": "roodio.toggleAutoApprove",
				"key": "cmd+alt+a",
				"mac": "cmd+alt+a",
				"win": "ctrl+alt+a",
				"linux": "ctrl+alt+a"
			}
		],
		"submenus": [
			{
				"id": "roodio.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "roodio.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"roodio.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"roodio.deniedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.deniedCommands.description%"
				},
				"roodio.commandExecutionTimeout": {
					"type": "number",
					"default": 0,
					"minimum": 0,
					"maximum": 600,
					"description": "%commands.commandExecutionTimeout.description%"
				},
				"roodio.commandTimeoutAllowlist": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.commandTimeoutAllowlist.description%"
				},
				"roodio.preventCompletionWithOpenTodos": {
					"type": "boolean",
					"default": false,
					"description": "%commands.preventCompletionWithOpenTodos.description%"
				},
				"roodio.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"roodio.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				},
				"roodio.enableCodeActions": {
					"type": "boolean",
					"default": true,
					"description": "%settings.enableCodeActions.description%"
				},
				"roodio.autoImportSettingsPath": {
					"type": "string",
					"default": "",
					"description": "%settings.autoImportSettingsPath.description%"
				},
				"roodio.maximumIndexedFilesForFileSearch": {
					"type": "number",
					"default": 10000,
					"minimum": 5000,
					"maximum": 500000,
					"description": "%settings.maximumIndexedFilesForFileSearch.description%"
				},
				"roodio.useAgentRules": {
					"type": "boolean",
					"default": true,
					"description": "%settings.useAgentRules.description%"
				},
				"roodio.apiRequestTimeout": {
					"type": "number",
					"default": 600,
					"minimum": 0,
					"maximum": 3600,
					"description": "%settings.apiRequestTimeout.description%"
				},
				"roodio.newTaskRequireTodos": {
					"type": "boolean",
					"default": false,
					"description": "%settings.newTaskRequireTodos.description%"
				},
				"roodio.codeIndex.embeddingBatchSize": {
					"type": "number",
					"default": 60,
					"minimum": 1,
					"maximum": 200,
					"description": "%settings.codeIndex.embeddingBatchSize.description%"
				},
				"roodio.debug": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debug.description%"
				},
				"roodio.debugProxy.enabled": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debugProxy.enabled.description%",
					"markdownDescription": "%settings.debugProxy.enabled.description%"
				},
				"roodio.debugProxy.serverUrl": {
					"type": "string",
					"default": "http://127.0.0.1:8888",
					"description": "%settings.debugProxy.serverUrl.description%",
					"markdownDescription": "%settings.debugProxy.serverUrl.description%"
				},
				"roodio.debugProxy.tlsInsecure": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debugProxy.tlsInsecure.description%",
					"markdownDescription": "%settings.debugProxy.tlsInsecure.description%"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint . --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"pretest": "turbo run bundle --cwd ..",
		"test": "vitest run",
		"format": "prettier --write .",
		"bundle": "node esbuild.mjs",
		"build:packages": "cd packages/types && npm install && npm run build && cd ../build && npm install && npm run build",
		"build": "npm run clean && npm run build:webview && npm run bundle",
		"build:all": "npm run build:packages && npm run build",
		"build:webview": "cd webview-ui && npm install && npm run build",
		"postinstall": "npm run build:packages",
		"vscode:prepublish": "npm run build",
		"vsix": "mkdirp ../bin && vsce package --no-dependencies --out ../bin",
		"publish:marketplace": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
		"watch:bundle": "node esbuild.mjs --watch",
		"watch:tsc": "cd .. && tsc --noEmit --watch --project src/tsconfig.json",
		"clean": "rimraf dist src/dist logs mock .turbo"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.922.0",
		"@aws-sdk/credential-providers": "^3.922.0",
		"@dotenvx/dotenvx": "^1.51.4",
		"@google/genai": "^1.29.1",
		"@lmstudio/sdk": "^1.1.1",
		"@mistralai/mistralai": "^1.9.18",
		"@modelcontextprotocol/sdk": "^1.25.1",
		"@qdrant/js-client-rest": "^1.14.0",
		"@roo-code/cloud": "file:./packages/cloud",
		"@roo-code/core": "file:./packages/core",
		"@roo-code/ipc": "file:./packages/ipc",
		"@roo-code/telemetry": "file:./packages/telemetry",
		"@roo-code/types": "file:./packages/types",
		"@vscode/codicons": "^0.0.36",
		"async-mutex": "^0.5.0",
